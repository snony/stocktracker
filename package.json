    "fuse.js": "^3.2.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-lines-ellipsis": "^0.13.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.5",
    "recharts": "^1.2.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "ress": "^1.2.2",
