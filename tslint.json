{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "no-console": false,
    "import-name": false
  },
  "linterOptions": {
    "exclude": ["config/**/*.js", "/**/*.js", "node_modules/**/*.ts"]
  }
}
